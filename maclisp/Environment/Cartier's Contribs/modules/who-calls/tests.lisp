;;; -*- package: CL-USER -*-;;;;;;; Tests for Who-Calls;;;(in-package "CL-USER")(defvar *bam*  )(defmacro bar ()  )(defun baz ()  );;;;;;; Guinea Pigs;;;(defconstant hoo  (function    (lambda ()      *bam*)))(defvar foo  (function    (lambda ()      (bar))))(defparameter goo  (function    (lambda ()      (baz))))(defstruct foo  (x *bam*)  (y (bar))  (z (baz)))(defclass goo ()    ((x :initform *bam*)     (y :initform (bar))     (z :initform (baz))))(defmacro foo ()  (list 'quote        (list *bam*              (bar)              (baz))))(defun goo ()  (list *bam*        (bar)        (baz)))(defgeneric hoo ()  (:method ()     (list (bar)           (baz))))(defmethod joo ((w window))  (list *bam*        (bar)))(defmethod joo :before ((w window))  (list (bar)))(defmethod joo :after ((w window))  (list (baz)))(defmethod joo :around ((w window))  (list (bar)        (baz)))(defmethod koo ((self window) x)  (list self x (bar)))