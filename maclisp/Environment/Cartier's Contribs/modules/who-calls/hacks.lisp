;;; -*- package: CC -*-;;;;;;; Who-Calls compiler hacks;;;(in-package "CC");;; This is where we hack the compiler(advise ccl::nx1-note-fcell-ref        (note-reference (car arglist) :who-calls)        :when :before)(advise ccl::nx1-note-vcell-ref        (note-reference (car arglist) :who-references)        :when :before);;; This is where we hack macro expansions(setq *macroexpand-hook*      'note-macro-expansion)