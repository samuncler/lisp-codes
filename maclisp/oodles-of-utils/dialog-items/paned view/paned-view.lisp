(in-package :oou)(oou-provide :paned-view);*****************************************************************                                    ;; Copyright © 1991 Institut fuer Informatik, University of Basel, Switzerland ; All Rights Reserved;; Author: Dieter Holz;; paned view and paned window.; ; ;; Changes (worth to be mentioned):; ------------------------------; none ;;*****************************************************************;*****************************************************************(oou-dependencies :paned-view-svm                              )(export '(unibas-paned-window unibas-paned-view));--------------------------------------------------------------------------(defclass unibas-paned-window (unibas-reshape-mixin window)   ());--------------------------------------------------------------------------(defclass unibas-paned-view (unibas-reshape-mixin view)   ())#|(make-instance 'unibas-paned-window                          :window-title "Simple Resizable Window"                          :view-position (list :top 40)                          :view-size #@(300 500)                          :window-type :document-with-zoom                          :subview-description                          (list (list (make-instance 'editable-text-dialog-item                                                                    :view-position #@(10 10)                                                                    :view-size #@(120 220))                                           :adjust-h                                          :stick-on-topleft)                                  (list (make-instance 'editable-text-dialog-item                                                                    :view-position #@(150 10)                                                                    :view-size #@(120 220))                                           :constant                                          :stick-on-topright)                                  (list (make-instance 'editable-text-dialog-item                                                                    :view-position #@(10 260)                                                                    :view-size #@(120 220))                                           :adjust-v                                          :stick-on-bottomleft)                                  (list (make-instance 'editable-text-dialog-item                                                                    :view-position #@(150 260)                                                                    :view-size #@(120 220))                                           :adjust-h-v                                          :stick-on-bottomright)))|#